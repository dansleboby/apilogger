{
    "require": {
        "php": "^8.0|^8.1",
        "ext-libxml": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "AWT\\": "src/"
        }
    },
    "name": "awt/apilogger",
    "description": "Small laravel package for viewing api logs which can be used in debugging.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "aung win thant",
            "email": "agwinthant@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "AWT\\Providers\\ApiLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "7.0",
        "nunomaduro/larastan": "^2.0"
    }
}
